{#
    Aqu√≠ se importa el componente de twig.
    Y se le pasan los datos con with { }
#}

<!doctype html>
<html lang="en">
<head>

  <meta charset="UTF-8" />
  <title>{{ title }}</title>
  <meta name="description" content="{{ description }}">
  <link rel="stylesheet" type="text/css" href="../../css/main.css" media="screen">
</head>

{% block body %}
    {% include '../flat-components/card.twig' with {
        title: title,
        subtitle: subtitle,
        desc: desc,
        url: url
    }  only %}
{% endblock %}

